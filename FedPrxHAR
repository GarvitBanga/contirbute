        "import tensorflow.keras.backend as kb\n",
        "class CustomAccuracy(tf.keras.losses.Loss):\n",
        "  def __init__(self):\n",
        "    super().__init__()\n",
        "  def call(self, y_true, y_pred):\n",
        "    # scce = tf.keras.losses.SparseCategoricalCrossentropy()\n",
        "    # scce=scce(y_true, y_pred)\n",
        "    proximal_term = 0.0\n",
        "    mu=0.001\n",
        "    \n",
        "    # studen=STUDENT.build()\n",
        "    # tchr=TEACHER.build()\n",
        "    # iterate through the current and global model parameters\n",
        "    # local_model1=MODEL.build()\n",
        "    # local_model1.set_weights(global_model.get_weights())\n",
        "\n",
        "    # for w,w_t in zip(local_model.get_weights(),global_model.get_weights()):\n",
        "\n",
        "    # print(\"WTL\",wtl[0])\n",
        "    # print(\"WTG\",wtg[0])\n",
        "\n",
        "    for w,w_t in zip(wtl,wtg):\n",
        "      # w=student.get_weights()\n",
        "      # w_t=teacher.get_weights()\n",
        "      # update the proximal term \n",
        "      #proximal_term += torch.sum(torch.abs((w-w_t)**2))\n",
        "      proximal_term += np.linalg.norm(w-w_t)\n",
        "    print(\"proximal term\",proximal_term)\n",
        "    return kb.sparse_categorical_crossentropy(y_true, y_pred,from_logits=False)+(mu/2)*proximal_term\n",
        "    \n",
        "    # return scce #+proximal_term\n",
        "\n"
      ]
    },
