        "from tensorflow.keras.layers import BatchNormalization\n",
        "class MODEL:\n",
        "    @staticmethod\n",
        "    def build():\n",
        "        model = Sequential()\n",
        "        model.add(Conv1D(128, 3,strides=2, padding=\"same\",input_shape=(n_timesteps, n_features)))\n",
        "        model.add(LeakyReLU(alpha=0.2))\n",
        "        model.add(MaxPooling1D(pool_size=2, strides=1, padding=\"same\"))\n",
        "        model.add(Dropout(0.25))\n",
        "\n",
        "\n",
        "        model.add(Conv1D(8, 3, strides=2, padding=\"same\") )\n",
        "        model.add(Conv1D(16, 3, strides=2, padding=\"same\") )        \n",
        "        model.add(Conv1D(32, 3, strides=2, padding=\"same\") )\n",
        "        model.add(Conv1D(64, 3, strides=2, padding=\"same\") )\n",
        "\n",
        "        model.add(Flatten())\n",
        "        # model.add(Dense(256/nl, activation='relu'))\n",
        "        # model.add(Dense(64/nl, activation='relu'))\n",
        "        model.add(Dense(6,activation='softmax'))#for output layer\n",
        "        # model = Sequential()\n",
        "        # model.add(Conv1D(32/nl, 3, activation='relu', kernel_initializer='he_uniform', padding='same', input_shape=(n_timesteps, n_features)))\n",
        "        # model.add(BatchNormalization())\n",
        "        # model.add(Conv1D(32/nl, 3, activation='relu', kernel_initializer='he_uniform', padding='same'))\n",
        "        # model.add(BatchNormalization())\n",
        "        # model.add(MaxPool1D(2))\n",
        "        # model.add(Dropout(0.2))\n",
        "        # model.add(Conv1D(64/nl, 3, activation='relu', kernel_initializer='he_uniform', padding='same'))\n",
        "        # model.add(BatchNormalization())\n",
        "        # model.add(Conv1D(64/nl, 3, activation='relu', kernel_initializer='he_uniform', padding='same'))\n",
        "        # model.add(BatchNormalization())\n",
        "        # model.add(MaxPool1D(2))\n",
        "        # model.add(Dropout(0.3))\n",
